  "problems.showCurrentInStatus": true,
  "notebook.formatOnSave.enabled": true,
  "notebook.lineNumbers": "on",
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false,
    "rust": false
  },
  "editor.tabSize": 4,
  "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
  "vscode-neovim.compositeKeys": {
    "jj": {
      "command": "vscode-neovim.escape"
    }
  },
  "testing.openTesting": "neverOpen",
  // "testing.gutterEnabled": false, // ネイティブUIのアイコンを非表示
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.fixAll,ruff": "explicit",
      "source.organizeImports.ruff": "explicit"
    },
    "editor.defaultFormatter": "charliermarsh.ruff",
  },
  "python.languageServer": "Pylance",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  // pytest実行時の引数設定を追加(--cov: カバレッジ計測の有効化 --cov-report xml: カバレッジ計測レポートをcoverage.xmlで出力)
  "python.testing.pytestArgs": ["tests", "--cov=src", "--cov-report", "xml"],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "coverage-gutters.showGutterCoverage": true, // ガター(ブレークポイント等が表示される場所)でのカバレッジ表示有無
  "coverage-gutters.showLineCoverage": true, // エディタ行でのカバレッジ表示有無
  "coverage-gutters.showRulerCoverage": true, // ルーラーでのカバレッジ表示有無
  // "python.testing.autoTestDiscoverOnSaveEnabled": true, // 保存時に自動でテストを実行
  // "pythonTestExplorer.testFramework": "pytest",
  "python.analysis.typeCheckingMode": "basic",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "rust-analyzer.checkOnSave": true,
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.check.extraArgs": ["--", "-A", "clippy::needless_return"],
  "rust-analyzer.interpret.tests": true,
  "rust-analyzer.testExplorer": true,
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "dart.lineLength": 119,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools",
  },
  "C_Cpp.default.cppStandard": "c++23",
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  "cmake.configureOnOpen": true,
  "C_Cpp.clang_format_style": "{ BasedOnStyle: Google, IndentWidth: 4, IndentAccessModifiers: true, ReflowComments: false, DerivePointerAlignment: false, PointerAlignment: Left, ColumnLimit: 0 }",
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, IndentAccessModifiers: true, ReflowComments: false, DerivePointerAlignment: false, PointerAlignment: Left, ColumnLimit: 0 }",
  "C_Cpp.default.includePath": [
    "${workspaceFolder}/**",
  ],
  "[log]": {
    "editor.wordWrap": "off"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[csv]": {
    "editor.wordWrap": "off"
  },
